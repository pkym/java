	}

	public static void kkn() {
		System.out.println("*******강규남 등장***************            \r\n"
				+ "          :##$##$;                                \r\n"
				+ "         ,$###$##$~                               \r\n"
				+ "         =$$=$$####!                              \r\n"
				+ "         $#===#####=                              \r\n"
				+ "         $!---::!##=          강규남                \r\n"
				+ "         $:;:~;,,,$!          나이: ??             \r\n"
				+ "         =(@)--(@)=;          직책: 3반 담임         \r\n"
				+ "         ..,,/....;~          특징: 만성피로          \r\n"
				+ "         ,....,..,,-                              \r\n"
				+ "          -:;O-~-,,.                              \r\n"
				+ "          -~~~-~-..                               \r\n"
				+ "          -----,,.                                \r\n"
				+ "          .-~~---,                                \r\n"
				+ "           --~-~~,~                               \r\n"
				+ "           !;!!;;;:.                              \r\n"
				+ "           ;!**!**;-                              \r\n"
				+ "           !!===!!;;~                             \r\n"
				+ "        .,,;**=*!;;:---,                          \r\n"
				+ "    .-~~-,-!***!*;:-,,--,.                        \r\n"
				+ "   .~----~~;;;;!!:-,-,-,,,.                       \r\n"
				+ "   -~~~~-~~~:;;;~-------,--.                      \r\n"
				+ "  .~:~-~-~---~:~-----,~--:-,                      \r\n"
				+ "  .~::-----~------------:;-.                      \r\n" + "  -:!~---------------~-;:,..     ");
	}

