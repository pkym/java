package javapractice;

import java.util.Random;
import java.util.Scanner;

public class ex2홀수 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("입력 : ");
		int N = sc.nextInt();
		int num=1;
		for(int i=0;i<N;i++) {
			num*=i+1;
		}
		System.out.println(num);
		
		
		
	}

}