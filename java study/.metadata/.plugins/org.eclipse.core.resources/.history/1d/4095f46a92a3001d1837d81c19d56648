package javaExam;

public class Question {
	
	
	
	public static void main(String[] args) {
		Game game1 = new Zelda();
		Mario game2 = new Mario();
		JustDance game3 = new JustDance();			
		
		insert(game1);
		insert(game2);
		insert(game3);
		
		game1.start();
		
	}

	
	public static void insert(Game game) {	
		
		game.start();
	
		
	}

	

	
}
